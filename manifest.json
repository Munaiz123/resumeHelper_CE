{
  "manifest_version": 2,
  "name": "Resume Helper",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "./assets/r.png",
    "default_popup": "./assets/popup.html",
    "default_title": "A popup will come here"
  }
}
